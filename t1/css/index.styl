font_black = #1c1c1c

body
    font-size 14px
    font-family Microsoft YaHei
    width 100%
    min-height 100%
    height auto
    //background-color pink
    .top_div_outer
        width 100%
        font-size 18px
        .top_div_inner
            max-width 1000px
            margin-left calc((100% - 1000px) / 2)
            color #3e97f8
            display flex
            flex-direction row
            justify-content space-between
            align-items center
            .second
                font-weight 700
                display flex
                flex-direction row
                justify-content flex-start
                align-items center
                img
                    margin-right 10px
                .end
                    margin-left 60px

    .blue_01
        width 100%
        height 370px
        background -webkit-radial-gradient(30% 50%, farthest-corner, #0b1e5d, #000E3F); /* Safari 5.1 - 6.0 */
        background -o-radial-gradient(30% 50%, farthest-corner, #0b1e5d, #000E3F); /* Opera 11.6 - 12.0 */
        background -moz-radial-gradient(30% 50%, farthest-corner, #0b1e5d, #000E3F); /* Firefox 3.6 - 15 */
        background radial-gradient(30% 50%, farthest-corner, #0b1e5d, #000E3F); /* 标准的语法 */
        background url("../image/blue_bg.jpg") no-repeat center center fixed
        background-size 100%
        .blue_div_inner
            max-width 1000px
            margin-left calc((100% - 1000px) / 2)
            height 100%
            display flex
            flex-direction row
            justify-content flex-start
            align-items center
            position relative
            img
                height 100%
                width 100%
            button
              position absolute
              left 110px
              top 248px
              margin-left -25px
              font-size 18px
              color #caceea
              border 1px solid #7f82a4
              background-color #000E3F
              padding 8px 40px
              border-radius 20px
              outline none
          button:hover
            cursor pointer
            color #00053f
            background-color #7f82a4

    .white_01
        width 100%
        height 630px
        font-weight 700
        color font_black
        .white_div_inner
            padding-left 30px
            max-width 1000px
            margin-left calc((100% - 1000px) / 2)
            height 100%
            .nav
                font-size 36px
                padding 75px 39px
                text-align center
            .second
                font-size 18px
                display flex
                flex-direction row
                justify-content flex-start
                align-items center
                >div
                  .b_01
                    display block
                  .b_02
                    display none
                .six
                    position relative
                    .text
                        text-align center
                        position absolute
                        z-index 99
                        top 75px
                        left 65px
                    .text_2
                        text-align center
                        position absolute
                        z-index 99
                        top 265px
                        left 65px
                    .text,.text_2:hover
                        cursor pointer
                    .repair
                        left 45px
                    span
                        display block
                .l_02, .r_02
                    display flex
                    flex-direction column
                    justify-content center
                    align-items center
                    margin-left -45px
                .mid
                    display flex
                    flex-direction column
                    justify-content center
                    align-items center
                    margin-left -45px
                .r_01
                    display flex
                    flex-direction column
                    justify-content center
                    align-items center
                    margin-left -45px
            .third
                text-align center
                padding-bottom 30px
                button
                    margin-left -25px
                    font-size 18px
                    color #8cb4e0
                    border 1px solid #8cb4e0
                    background-color #fff
                    padding 8px 40px
                    border-radius 20px
                    outline none
                button:hover
                    background-color #8cb4e0
                    color #fff
                    cursor pointer
    .blue_02
        width 100%
        height 460px
        background-color #000a67
        //background url("../image/blue_bg.jpg") no-repeat center center fixed
        //background-size 100%
        .blue_div_inner
            max-width 1000px
            margin-left calc((100% - 1000px) / 2)
            height 100%
            display flex
            flex-direction row
            justify-content flex-start
            align-items center
            .left
                width 35%
                display flex
                flex-direction column
                justify-content flex-end
                align-items center
                img
                    margin-top 120px
                    margin-right 105px
            .right
                height 100%
                width 65%
                .title
                    font-size 36px
                    color #9acbf9
                    text-align left
                    padding 90px 0 16px 0
                .line
                    ul
                        list-style none
                        margin 0
                        padding 0
                        li
                            font-size 14px
                            display flex
                            flex-direction row
                            justify-content flex-start
                            align-items baseline
                            padding 8px 0
                            div
                                font-size 14px
                                //color #ffff
                                background-color #9acbf9
                                width 24px
                                height 24px
                                min-height 24px
                                //padding calc((20px - 14px) / 2) 0
                                //text-align center
                                border-radius 16px
                                display flex
                                flex-direction row
                                justify-content center
                                align-items center
                                margin-right 10px
                                .num
                                    color #ffff
                                    display block
                                    width 1em
                                    text-align center
                                    font-weight 700
                            span
                                width calc(100% - 24px)
                                color #fff
                                word-break break-all
                                white-space normal
                                flex-wrap wrap
                                line-height 20px

