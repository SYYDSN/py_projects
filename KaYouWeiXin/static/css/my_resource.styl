.top, .middle, .bottom
    background-color #F4F2F3
    width 100%
    margin-bottom 10px
    padding 10px 0
    >h3
        text-align center

.top
    height 70px

.middle
    height auto
    .user_list
        width 100%
        .line
            display flex
            flex-direction row
            justify-content center
            align-items center
            height 40px
            padding 5px 10px
            .cell
                width 33.33%
                display flex
                flex-direction row
                justify-content center
                align-content center
                >img
                    height 30px
                    width 30px
                    border-radius 4px

.bottom
    height 60px
    .page_area
        height 100%
        font-size 16px
        display flex
        flex-direction row
        justify-content center
        align-items center
        >a
            clor lightgrey
            padding 5px 10px
        .active
            color #337ab7
