font_black = #1c1c1c

body
    font-size 14px
    font-family Microsoft YaHei
    width 100%
    min-height 100%
    height auto
    //background-color pink
    .my_button
        margin-left -25px
        font-size 18px
        color #8cb4e0
        border 1px solid #8cb4e0
        background-color #fff
        padding 8px 40px
        border-radius 20px
        outline none
    .my_button:hover
        background-color #8cb4e0
        color #fff
        cursor pointer

    .top_div_outer
        width 100%
        font-size 18px
        .top_div_inner
            max-width 1000px
            margin-left calc((100% - 1000px) / 2)
            color #3e97f8
            display flex
            flex-direction row
            justify-content space-between
            align-items center
            .second
                font-weight 700
                display flex
                flex-direction row
                justify-content flex-start
                align-items center
                img
                    margin-right 10px
                .end
                    color #3e97f8
                    margin-left 60px
                    text-decoration none

    .blue_01
        width 100%
        height 370px
        background -webkit-radial-gradient(30% 50%, farthest-corner, #0b1e5d, #000E3F); /* Safari 5.1 - 6.0 */
        background -o-radial-gradient(30% 50%, farthest-corner, #0b1e5d, #000E3F); /* Opera 11.6 - 12.0 */
        background -moz-radial-gradient(30% 50%, farthest-corner, #0b1e5d, #000E3F); /* Firefox 3.6 - 15 */
        background radial-gradient(30% 50%, farthest-corner, #0b1e5d, #000E3F); /* 标准的语法 */
        background url("../image/blue_bg.jpg") no-repeat center center fixed
        background-size 100%
        .blue_div_inner
            max-width 1000px
            margin-left calc((100% - 1000px) / 2)
            height 100%
            display flex
            flex-direction row
            justify-content flex-start
            align-items center
            position relative
            img
                height 100%
                width 100%
            button
              position absolute
              left 110px
              top 248px
              margin-left -25px
              font-size 18px
              color #caceea
              border 1px solid #7f82a4
              background-color #000E3F
              padding 8px 40px
              border-radius 20px
              outline none
          button:hover
            cursor pointer
            color #00053f
            background-color #7f82a4

