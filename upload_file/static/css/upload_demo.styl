
.my_div
  min-height 400px
  background-color #f7f7f7
  display flex
  flex-direction column
  justify-content center
  align-items center
  .json_div
    min-width 100%
    padding 10px
    >.json_div_inner
      min-width 100%
      min-height 50px
      border 1px solid lightgrey
      border-radius 5px
      padding 10px
      >li
        margin-left 20px
  #my_progress
    width 96%
    position relative
    >.my_per
      color #ff7d7f
      position absolute
      display block
      width 4em
      top 1px
      left 50%

  .my_bottom
    padding 10px 10px 20px
    >#select_image
      display none